  /usr/share/doc/ggz-utils-0.0.14.1/INSTALL
  /usr/share/doc/ggz-utils-0.0.14.1/NEWS
  /usr/share/doc/ggz-utils-0.0.14.1/README
  /usr/share/ggzmetaserv/metaservconf.xml
  /usr/share/man/man1/ggz-cmd.1.gz
  /usr/share/man/man1/ggzcommgen.1.gz
  /usr/share/man/man1/ggzmetaserv.1.gz
  /usr/share/man/man1/telggz.1.gz

